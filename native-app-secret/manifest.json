{
  "name": "Secret",
  "description": "An extension that provides private state outside of Chrome",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["nativeMessaging"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["secret.js"],
      "world": "MAIN"
    }
  ]
}
